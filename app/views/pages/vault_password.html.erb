<h4>Enter Vault Password</h4>
<%= form_tag( pages_update_data_path, method: "get", :class => "form" ) do %>
    <div class="form-group">
	    <%= password_field_tag(:vault_password, nil, :class => "form-control", :placeholder => "Vault Password") %>
	</div>
	<div>
		<%= submit_tag "Fetch Data", class: "btn btn-success" %>
	</div>
<% end %>

<script>
$( document ).ready(function() {
  $( "#vault_password" ).focus();
});
</script>