<tr id="contactID<%= contact.id %>" >
	<td>
		<%= link_to contacts_path(:contact => contact.id) do %>
			<h4><%= contact.fullname %></h4>
		<% end %>
	</td>
	<td>
		<h4><%= render "/touchpoints/partials/#{touchpoint.name}_#{touchpoint.direction}_current_user", :subject => touchpoint.subject %>
		</h4>
	</td>
	<td>
		<%= link_to toggle_contact_as_actionable_contact_path(contact.id, :make_actionable => make_actionable), :data => {:remote => true} do %>
			<h4><span class="glyphicon glyphicon-<%= glyphicon %>"></span></h4>
		<% end %>
	</td>
</tr>
